    <li><label><p>Parent Contact:</p>
      <input type="text" name="contact">
    </label></li>

    <li><label><p>Goals:</p>
      <input type="text" name="goals[]">
      <input type="text" name="goals[]">
      <input type="text" name="goals[]">
      <input type="text" name="goals[]">
      <input type="text" name="goals[]">
